package com.tss.ecommers;

public class FestivalTest {

	public static void main(String[] args) {
		FixedDeposit fixedDeposite1 = new FixedDeposit(1, "Ram", 4000, 6, FestialEnum.NEWYEAR);
		FixedDeposit fixedDeposite2 = new FixedDeposit(2, "Shyam", 5000, 5, FestialEnum.DIWALI);

		System.out.println("FD1 Interest: " + fixedDeposite1.calculateSimpleInterest());
		System.out.println("FD2 Interest: " + fixedDeposite2.calculateSimpleInterest());
	}

}
