package com.tss.builder;

public class Home {
	private int bedRooms;
	private int bathRooms;
	private int DrawRooms; // optional
	private int Halls;

	public int getBedRooms() {
		return bedRooms;
	}

	public int getBathRooms() {
		return bathRooms;
	}

	public int getDrawRooms() {
		return DrawRooms;
	}

	public int getHalls() {
		return Halls;
	}



	private Home(int bedRooms, int bathRooms, int drawRooms, int halls) {
		super();
		this.bedRooms = bedRooms;
		this.bathRooms = bathRooms;
		DrawRooms = drawRooms;
		Halls = halls;
	}

}
