package com.tss.ecommers;

public class FestivalTest {

	public static void main(String[] args) {
		FixedDeposit fd1 = new FixedDeposit(1, "Ram", 4000, 6, FestialEnum.NEWYEAR);
		FixedDeposit fd2 = new FixedDeposit(2, "Shyam", 5000, 5, FestialEnum.DIWALI);

		System.out.println("FD1 Interest: " + fd1.calculateSimpleInterest());
		System.out.println("FD2 Interest: " + fd2.calculateSimpleInterest());
	}

}
