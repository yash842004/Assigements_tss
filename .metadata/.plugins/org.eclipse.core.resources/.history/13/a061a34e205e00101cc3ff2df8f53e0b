package com.tss.prototype;

import java.io.Serializable;

public abstract class Vehical implements Cloneable , Serializable{
	private String model;
	private String color;
	private Engine engine;
	
	public Vehical(String model, String color, Engine engine) {
		
		super();
		this.model = model;
		this.color = color;
		this.engine = engine;
	}
	public String getModel() {
		return model;
	}
	public String getColor() {
		return color;
	}
	public Engine getEngine() {
		return engine;
	}
	public void setModel(String model) {
		this.model = model;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public void setEngine(String engine) {
		this.engine = engine;
	}
    public abstract void drive();
	
	public Object cloan() throws CloneNotSupportedException {
		return (Vehical) super.clone();
	}

	

}
