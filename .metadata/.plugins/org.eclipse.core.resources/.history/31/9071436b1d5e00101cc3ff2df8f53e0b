package com.tss.prototype;

import java.io.Serializable;

public class Engine implements Cloneable,Serializable  {
	private String type;
	private int horshpower;

	public Engine(String type, int horshpower) {
		super();
		this.type = type;
		this.horshpower = horshpower;
	}

	public String getType() {
		return type;
	}

	public int getHorshpower() {
		return horshpower;
	}

	public void setType(String type) {
		this.type = type;
	}

	public void setHorshpower(int horshpower) {
		this.horshpower = horshpower;
	}
	
	public Object cloan() throws CloneNotSupportedException {
		return super.clone();
	}

}
