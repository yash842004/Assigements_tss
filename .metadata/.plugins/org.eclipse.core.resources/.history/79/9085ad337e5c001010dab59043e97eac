package com.tss.model.violation;

public class TaxCalculator extends DBLoggger{
	
	dblogger : DBLogger;
	
	void TaxCalculator() {
		
	}
	
	public void calculateTax(int amount , int rate) {
		int tax = 0;
		try {
			
			tax = amount / rate;
			System.out.println(tax);
		}catch(Exception e) {
			System.out.println();
			
		}
	}

}
